################################################################################
#
# Theme:
# <%= theme %>
#
# Converted using https://github.com/andrius/manjaro-sway-themes
# Author: twitter.com/@andrius_kai
#
# Installation instructions:
# edit your foot.ini (~/config/foot/foot.ini) and replace both
# [cursor] and [colors] sections.
#
# Browse following repository for more colors
# https://codeberg.org/dnkl/foot/src/branch/master/themes
#
################################################################################

[cursor]
color=<%= colors[:cursor_color] %> <%= colors[:cursor_text_color] %>

[colors]
alpha=1.0

foreground=<%= colors[:foreground_color] %>
background=<%= colors[:background_color] %>

regular0=<%= colors[:ansi_0_color] %>
regular1=<%= colors[:ansi_1_color] %>
regular2=<%= colors[:ansi_2_color] %>
regular3=<%= colors[:ansi_3_color] %>
regular4=<%= colors[:ansi_4_color] %>
regular5=<%= colors[:ansi_5_color] %>
regular6=<%= colors[:ansi_6_color] %>
regular7=<%= colors[:ansi_7_color] %>
bright0=<%= colors[:ansi_8_color] %>
bright1=<%= colors[:ansi_9_color] %>
bright2=<%= colors[:ansi_10_color] %>
bright3=<%= colors[:ansi_11_color] %>
bright4=<%= colors[:ansi_12_color] %>
bright5=<%= colors[:ansi_13_color] %>
bright6=<%= colors[:ansi_14_color] %>
bright7=<%= colors[:ansi_15_color] %>

selection-foreground=<%= colors[:selected_text_color] %>
selection-background=<%= colors[:selection_color] %>
