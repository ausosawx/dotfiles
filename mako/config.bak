font=Sauce Code Pro Medium Nerd Font Complete Mono 13
on-notify=exec mpv /usr/share/sounds/freedesktop/stereo/message.oga
icon-path=/usr/share/icons/Vimix-Beryl
layer=top
markup=1
anchor=top-right
background-color=#1E1E28
text-color=#DADAE8
width=400
height=200
max-icon-size=96
icon-location=right
margin=20,20,0
padding=15,15,15
border-size=2
border-color=#a4b9ef
border-radius=15
progress-color=over #332E41
default-timeout=10000
group-by=summary

[mode=do-not-disturb]
invisible=1

[urgency=low]
border-color=#4d79ff

[urgency=normal]
border-color=#0099ff

[urgency=high]
border-color=#00ffff



